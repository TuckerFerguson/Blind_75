public class Solution{
    static int[] twoSum(int[] array, int target){
        int size = array.length;
        for(int i=0;i<size;i++){
            for(int j=0;j<size;j++){
             
                if((array[i] + array[j]) == target){
                    int[] returnarry = {i,j};
                   
                    return returnarry;
                }
            }
        }
        return array;
    }
    public static void main(String[] args){
        int[] arry = {1,2,3,4};
        int target = 6 ;
        twoSum(arry,target);
    }
}